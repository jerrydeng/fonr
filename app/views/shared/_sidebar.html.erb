<ul id="sidebar-items">

  <li>
    <h3>Helper Methods</h3>
    <ul class="links">
      <% property(:helpers).each do |helper| %>
        <li><%= link_to helper.first, helper_path(helper.first) %></li>
      <% end %>
    </ul>
  </li>
  
  <li>
    <h3>Facebooker Links</h3>
    <ul class="links">
      <li><%= link_out_to 'Documentation', 'http://facebooker.rubyforge.org/' %></li>
      <li><%= link_out_to 'GitHub', 'http://github.com/mmangino/facebooker/tree/master' %></li>
      <li><%= link_out_to 'Mailing List', 'http://groups.google.com/group/facebooker/' %></li>
    </ul>
  </li>
  
</ul>
